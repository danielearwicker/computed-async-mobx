  },
  "devDependencies": {
    "@types/blue-tape": "^0.1.31",
    "@types/jsdom": "^11.0.2",
    "blue-tape": "^1.0.0",
    "coveralls": "^2.11.15",
    "faucet": "0.0.1",
    "istanbul": "^0.4.5",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "parcel": "^1.12.4",
    "typedoc": "^0.19.2"
